/* transpose of matrix*/
#include<stdio.h>
#include<conio.h>
int main()
{
	int m,n,i,j,matrix[10][10],transpose[10][10];
	printf("enter rows and columns:");
	scanf("%d%d",&m,&n);
	printf("enter the elements of matrix:");
	for(i=0;i<m;i++)
	for(j=0;j<n;j++)
	scanf("%d",&matrix[i][j]);
	for(i=0;i<m;i++)
	for(j=0;j<n;j++)
	transpose[j][i]=matrix[i][j];
	printf("transpose of the matrix:");
	for(i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		printf("%d\t",transpose[i][j]);
	}
	return 0;
}
